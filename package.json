    "pouchdb-collate": "~1.0.0"
  },
  "devDependencies": {
    "chai": "~1.8.1",
    "mocha": "~1.14.0",
    "istanbul": "~0.1.45",
    "pouchdb": "git://github.com/daleharvey/pouchdb.git#1387",
    "browserify": "~2.36.0",
    "jshint": "~2.3.0",
    "mocha-as-promised": "~2.0.0",
