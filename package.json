{
  "name": "pouchdb-mapreduce",
  "version": "2.2.3",
  "description": "Map Reduce =====",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/pouchdb/mapreduce.git"
  },
  "scripts": {
    "do_tests": "TEST_DB=testdb,http://localhost:5984/testdb istanbul test ./node_modules/mocha/bin/_mocha test/test.js",
    "jshint": "jshint -c .jshintrc *.js test/test.js",
    "test": "npm run jshint && npm run do_tests",
    "build": "mkdir -p dist && browserify index.js -s mapReduce -o dist/pouchdb-mapreduce.js",
    "dev": "browserify test/test.js > test/test-bundle.js && npm run dev-server",
    "dev-server": "./bin/dev-server.js",
    "coverage": "npm test --coverage && istanbul check-coverage --lines 100 --function 100 --statements 100 --branches 100"
  },
  "keywords": [
    "pouch",
    "pouchdb",
    "mapreduce",
    "couch",
    "couchdb"
  ],
  "author": "",
  "license": "Apache",
  "bugs": {
    "url": "https://github.com/pouchdb/mapreduce/issues"
  },
  "dependencies": {
    "lie": "^2.6.0",
    "pouchdb-collate": "~1.1.0",
    "inherits": "~2.0.1",
    "extend": "^1.2.1",
    "argsarray": "0.0.1",
    "md5-jkmyers": "0.0.1",
    "es3ify": "^0.1.3",
    "pouchdb-persistence": "0.0.1"
  },
  "devDependencies": {
    "bluebird": "^1.0.7",
    "browserify": "~2.36.0",
    "chai": "~1.8.1",
    "chai-as-promised": "~4.1.0",
    "corsproxy": "~0.2.13",
    "http-proxy": "~0.10.3",
    "http-server": "~0.5.5",
    "jshint": "~2.3.0",
    "mocha": "~1.18",
    "pouchdb": "pouchdb/pouchdb",
    "watchify": "~0.4.1",
    "istanbul": "^0.2.7"
  },
  "peerDependencies": {
    "pouchdb": "^2.1.0"
  },
  "browser": {
    "crypto": false
  },
  "browserify": {
    "transform": [
      "es3ify"
    ]
  }
}
