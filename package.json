  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/pouchdb/mapreduce.git"
  },
  "scripts": {
    "do_tests": "TEST_DB=testdb,http://localhost:5984/testdb ./node_modules/istanbul/lib/cli.js test ./node_modules/mocha/bin/_mocha test/test.js",
